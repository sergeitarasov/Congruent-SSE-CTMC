Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Makes", " ", "formulas", " ", "for", " ", "HiCLasse", " ", "model"}], 
   "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"makeHiClasse", "[", 
     RowBox[{
     "LamTensor_List", ",", "Mu_List", ",", "Q_List", ",", " ", "Ds_List", 
      ",", "Es_List"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "DEs", ",", "EEs", ",", "L1", ",", "L2", ",", "L3", ",", "Lambdas", 
        ",", "M2", ",", "M3", ",", "Mus", ",", "NoChnageLambdas", ",", "Deq", 
        ",", "Eeq"}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "Create", " ", "a", " ", "vector", " ", "of", " ", "ones", " ", "with",
         " ", "the", " ", "same", " ", "length", " ", "as", " ", "Mu"}], 
       "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"onesVector", "=", 
        RowBox[{"ConstantArray", "[", 
         RowBox[{"1", ",", 
          RowBox[{"Length", "[", "Mu", "]"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{"Calculate", " ", "DEs", " ", "and", " ", "EEs"}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"DEs", "=", 
        RowBox[{"Outer", "[", 
         RowBox[{"Times", ",", "Ds", ",", "Es"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"EEs", "=", 
        RowBox[{"Outer", "[", 
         RowBox[{"Times", ",", "Es", ",", "Es"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{"Make", " ", "lambdas"}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{"L1", "=", 
        RowBox[{"Map", "[", 
         RowBox[{"makeSymmetricMatrix", ",", "LamTensor", ",", 
          RowBox[{"{", "1", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"L2", "=", 
        RowBox[{"Map", "[", 
         RowBox[{"multiplyDiagonalByK", ",", "L1", ",", 
          RowBox[{"{", "1", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"L3", "=", 
        RowBox[{"Map", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"#", "*", "DEs"}], "&"}], ",", "L2", ",", 
          RowBox[{"{", "1", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"Lambdas", "=", 
        RowBox[{"Map", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"onesVector", ".", "#", ".", "onesVector"}], "&"}], ",", 
          "L3", ",", 
          RowBox[{"{", "1", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{"Make", " ", "extinctions"}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{"M2", "=", 
        RowBox[{"Map", "[", 
         RowBox[{"OffDiagonalHalve", ",", "L1", ",", 
          RowBox[{"{", "1", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"M3", "=", 
        RowBox[{"Map", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"#", "*", "EEs"}], "&"}], ",", "M2", ",", 
          RowBox[{"{", "1", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"Mus", "=", 
        RowBox[{"Map", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"onesVector", ".", "#", ".", "onesVector"}], "&"}], ",", 
          "M3", ",", 
          RowBox[{"{", "1", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{"No", " ", "changes"}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{"NoChnageLambdas", "=", 
        RowBox[{
         RowBox[{"-", "1"}], "*", 
         RowBox[{"Map", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"onesVector", ".", 
             RowBox[{"(", "#", ")"}], ".", "onesVector"}], "&"}], ",", 
           "LamTensor", ",", 
           RowBox[{"{", "1", "}"}]}], "]"}]}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{"E", " ", "and", " ", "D", " ", "probabilities"}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"Deq", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"NoChnageLambdas", "-", "Mu"}], ")"}], "*", "Ds"}], "+", 
         RowBox[{"Q", ".", "Ds"}], "+", "Lambdas"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Eeq", "=", 
        RowBox[{"Mu", "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"NoChnageLambdas", "-", "Mu"}], ")"}], "*", "Es"}], "+", 
         RowBox[{"Q", ".", "Es"}], "+", "Mus"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{"Return", " ", "results"}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"Eeq", ",", "Deq"}], "}"}]}]}], "\[IndentingNewLine]", 
     "]"}]}], "\n"}]}]], "Input",
